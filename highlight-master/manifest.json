{
  "manifest_version": 3,
  "name": "Highlight Master",
  "version": "1.0.0",
  "description": "Highlight and search from the data.",
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "search": {
      "suggested_key": {
        "default": "Ctrl+Q",
        "mac": "Command+Y"
      },
      "description": "Test"
    }
  },
  "permissions": [
    "storage",
    "notifications",
    "contextMenus"
  ]
}